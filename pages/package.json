{
  "name": "@jhb.software/payload-pages-plugin",
  "version": "0.1.13",
  "description": "Payload CMS plugin that adds essential fields for hierarchical page structure to collections.",
  "bugs": "https://github.com/jhb-software/payload-plugins/issues",
  "repository": "https://github.com/jhb-software/payload-plugins",
  "keywords": [
    "payload",
    "plugin",
    "pages"
  ],
  "author": "JHB Software",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "test": "jest",
    "lint": "eslint .",
    "prepublish": "tsc"
  },
  "dependencies": {},
  "peerDependencies": {
    "next": "15.0.0",
    "@payloadcms/ui": "^3.5.0",
    "payload": "^3.5.0",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "typescript": "5.7.2",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.1"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "main": "./dist/index.js",
    "registry": "https://registry.npmjs.org/",
    "types": "./dist/index.d.ts",
    "access": "public",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./client": {
        "import": "./dist/exports/client.js",
        "types": "./dist/exports/client.d.ts",
        "default": "./dist/exports/client.js"
      }
    }
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "default": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./client": {
      "import": "./src/exports/client.ts",
      "default": "./src/exports/client.ts",
      "types": "./src/exports/client.ts"
    }
  }
}
