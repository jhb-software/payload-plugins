{
  "name": "admin-search-dev",
  "description": "Dev app for the admin-search plugin.",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"${NODE_OPTIONS} --no-deprecation\" next dev",
    "dev:turbo": "pnpm dev --turbo",
    "dev:safe": "rm -rf .next && pnpm dev",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=./src/payload.config.ts payload",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=./src/payload.config.ts payload generate:types",
    "generate:schema": "payload-graphql generate:schema",
    "generate:importmap": "cross-env PAYLOAD_CONFIG_PATH=./src/payload.config.ts payload generate:importmap",
        "seed": "cross-env PAYLOAD_CONFIG_PATH=./src/payload.config.ts tsx seed-db.ts"
  },
  "dependencies": {
    "@payload-plugin/admin-search": "workspace:*",
    "@payloadcms/db-mongodb": "3.x",
    "@payloadcms/next": "3.x",
    "@payloadcms/plugin-search": "3.x",
    "@payloadcms/richtext-lexical": "3.x",
    "@payloadcms/translations": "3.x",
    "@payloadcms/ui": "3.x",
    "next": "15.x",
    "payload": "3.x",
    "react": "19.x",
    "react-dom": "19.x"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
        "tsx": "^4.7.0"
  }
}
