{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Payload dev",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["dev"],
      "cwd": "${workspaceFolder}/pages/dev",
      "console": "integratedTerminal",
      "protocol": "inspector",
      "sourceMaps": true,
      "env": { "NODE_OPTIONS": "--inspect=9229" }
    }
  ]
}